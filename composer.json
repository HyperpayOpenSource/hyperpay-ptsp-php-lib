{
  "name": "hyperbill/hyperpay-ptsp",
  "description": "Hyperpay PTSP PHP client to create payment links via Hyperbill API.",
  "type": "library",
  "version": "1.0.1",
  "keywords": ["payment", "hyperbill", "hyperpay", "ptsp", "payment-link", "sdk"],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "illuminate/http": "^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "HyperBill\\HyperpayPtsp\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HyperBill\\HyperpayPtsp\\HyperpayPtspServiceProvider"
      ],
      "aliases": {
        "Hyperpay": "HyperBill\\HyperpayPtsp\\Facades\\Hyperpay"
      }
    }
  }
}
